{
  "name": "web-switcher",
  "version": "0.5.1",
  "private": true,
  "description": "Build your UserScript with webpack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Krysl/WebSwitcher"
  },
  "author": {
    "name": "Krysl",
    "email": "krysl@qq.com"
  },
  "scripts": {
    "anylize": "cross-env npm_config_report=true npm run build",
    "build": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\"  webpack --mode production --config config/webpack.config.production.ts",
    "dev": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\"  webpack --mode development --config config/webpack.config.dev.ts",
    "lint": "eslint src -f visualstudio",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "dependencies": {
    "@svgdotjs/svg.js": "^3.1.1",
