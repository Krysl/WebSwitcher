{
  "name": "webpack-userscript-template",
  "description": "Build your UserScript with webpack",
  "version": "0.0.0",
  "author": {
    "name": "Krysl",
    "email": "krysl@qq.com"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "lint": "eslint src",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags",
    "anylize": "npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.js",
    "dev": "webpack --mode development --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/webpack-userscript-template"
  },
  "private": true,
  "dependencies": {
    "axios": "0.20.0",
    "axios-userscript-adapter": "0.0.7",
    "jquery": "3.5.1",
    "vue": "^3.0.5",
    "vue-class-component": "^8.0.0-0"
  },
  "devDependencies": {
    "@types/estree": "0.0.42",
    "@types/jquery": "^3.5.5",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "css-loader": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "^6.2.0",
    "less": "3.12.2",
    "less-loader": "7.0.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "1.2.1",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "userscript-metadata-webpack-plugin": "0.0.6",
    "vue-loader": "^16.1.2",
    "vue-style-loader": "^4.1.2",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-livereload-plugin": "2.3.0",
    "webpack-merge": "5.1.4"
  }
}
